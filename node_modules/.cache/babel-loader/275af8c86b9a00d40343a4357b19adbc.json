\n        this.$emit(\"gameOver\", \"draw\");\n      }\n    },\n    markGrid(id) {\n      this.grid[id] = this.turn;\n    },\n    showGrid() {\n      console.log(this.grid);\n    }\n  }\n};\n</script>\n\n<style>\n#container {\n  width: 50vw;\n  height: 50vw;\n  display: grid;\n  grid-template-columns: repeat(3, 1fr);\n  grid-template-rows: repeat(3, 1fr);\n}\n</style>"],"sourceRoot":"src/components"}]}